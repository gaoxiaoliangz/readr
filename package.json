  "private": true,
  "engines": {
    "node": ">=6.5",
    "npm": ">=3.10"
  },
  "scripts": {
    "start": "npm run devServer",
    "all": "concurrently \"npm run devServer\" \"npm run debug\" \"npm run watch:node\"",
    "serve": "node bin/www",
    "debug": "DEBUG=readr:* nodemon --debug=9229 --inspect bin/www",
    "debug:noReload": "DEBUG=readr:* node --debug=9229 --inspect bin/www",
    "debug:legacy": "DEBUG=readr:* nodemon --debug=5858 bin/www",
    "prebuild": "npm run clean",
    "build": "npm run task -- build --name=build --release",
    "build:es5": "BABEL_ENV=es5 babel build/es --out-dir build/es5 --source-maps inline",
    "build:js": "tsc --outDir build/js",
    "devServer": "npm run task -- devServer --name=run\\ dev\\ server",
    "getSchema": "npm run task -- getSchema",
    "bundle:dll": "npm run task -- bundle --target=dll --name=bundle\\ dll",
    "bundle:server": "npm run task -- bundle --target=server --name=bundle\\ server",
    "bundle:assets": "npm run task -- bundle --target=assets --name=bundle\\ assets --release",
    "clean": "npm run task -- cleanBuild --name=clean\\ build",
    "lint": "eslint src",
    "watch:server": "npm run task -- bundle --target=server --watch --name=watch\\ server",
    "watch:es": "tsc --watch",
    "watch:test": "npm test -- --watch",
    "test": "jest --no-cache",
    "task": "babel-node tools/runTask",
    "tsc": "tsc"
  },
